{
  "presets": ["module:@react-native/babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./app"],
        "alias": {
          "components": "./app/components",
          "configs": "./app/configs",
          "features": "./app/features",
          "hooks": "./app/hooks",
          "layouts": "./app/layouts",
          "types": "./app/types",
          "utils": "./app/utils"
        }
      }
    ]
  ]
}
